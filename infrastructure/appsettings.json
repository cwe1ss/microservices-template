{
  "keys": {
    "Template:SimpleKey": {
      "value": "Default value"
    },

    "Template:EnvironmentSpecific": {
      "value": "Default value",
      "environments": {
        "development": "DEV value"
      }
    },
    "Template:JsonKey": {
      "contentType": "application/json",
      "value": {
        "someField": "someValue"
      }
    }
  },

  "flags": {
    "Template:SimpleFlag": {
      "enabled": {
        "development": true,
        "production": false
      },
      "description": "A simple yes/no flag"
    },

    "Template:PercentageFlag": {
      "enabled": {
        "development": true
      },
      "description": "Valid for 50% of all requests",
      "conditions": {
        "client_filters": [
          {
            "name": "Microsoft.Percentage",
            "parameters": {
              "Value": "50"
            }
          }
        ]
      }
    },

    "Template:TargetPercentageFlag": {
      "enabled": {
        "development": true
      },
      "description": "Valid for 50% of all users",
      "conditions": {
        "client_filters": [
          {
            "name": "Microsoft.Targeting",
            "parameters": {
              "Audience": {
                "DefaultRolloutPercentage": 50
              }
            }
          }
        ]
      }
    },

    "Template:TargetGroupFlag": {
      "enabled": {
        "development": true
      },
      "description": "Valid only for a group",
      "conditions": {
        "client_filters": [
          {
            "name": "Microsoft.Targeting",
            "parameters": {
              "Audience": {
                "DefaultRolloutPercentage": 0,
                "Groups": [
                  {
                    "Name": "group1",
                    "RolloutPercentage": 100
                  }
                ]
              }
            }
          }
        ]
      }
    },

    "Template:TargetUsersFlag": {
      "enabled": {
        "development": true
      },
      "description": "Valid only for some users",
      "conditions": {
        "client_filters": [
          {
            "name": "Microsoft.Targeting",
            "parameters": {
              "Audience": {
                "DefaultRolloutPercentage": 0,
                "Users": [
                  "user1",
                  "user2"
                ]
              }
            }
          }
        ]
      }
    },

    "Template:TargetTimeFlag": {
      "enabled": {
        "development": true
      },
      "description": "Valid only for one day",
      "conditions": {
        "client_filters": [
          {
            "name": "Microsoft.TimeWindow",
            "parameters": {
              "Start": "2022-10-13T14:00:00.000Z",
              "End": "2022-10-14T14:00:00.000Z"
            }
          }
        ]
      }
    }
  }
}
